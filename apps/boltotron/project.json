      "executor": "nx:run-commands",
      "options": {
        "command": "yarn run tauri dev",
        "cwd": "apps/boltotron"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn run tauri dev",
        "cwd": "apps/boltotron"
      }
